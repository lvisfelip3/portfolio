---
import Layout from "@layouts/Layout.astro";
import Hero from "@layouts/Hero.astro";
import Navbar from "@layouts/Navbar.astro";
import Projects from "@layouts/Projects.astro";
import Contact from "@layouts/Contact.astro";
import References from '@layouts/References.astro';
import GoTop from "@/components/GoTop.astro";
import Footer from "@layouts/Footer.astro";
import NavbarMobile from "@layouts/NavbarMobile.astro";
---

<Layout title="Luis Dev Portfolio">
    <Navbar />
    <NavbarMobile />
    <main class="flex flex-col justify-center items-center lg:px-[460px] md:px-52 sm:px-36 px-12">
        <Hero />
        <Projects />
        <References />
        <Contact />
    </main>
    <GoTop />
    <Footer />
</Layout>
